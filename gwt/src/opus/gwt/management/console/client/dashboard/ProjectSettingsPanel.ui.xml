<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../resources/ProjectManager.css" type="opus.gwt.management.console.client.resources.ProjectManagerCss.ProjectManagerStyle"/>
	<ui:style field="form" src="../resources/Forms.css" type="opus.gwt.management.console.client.resources.FormsCss.FormsStyle" />
	
	<g:SimplePanel>
		<g:HTMLPanel styleName="{style.dashboardMainPanel}">
			<g:HTMLPanel styleName="{style.header}">
				<g:HTMLPanel styleName="{style.insetType}">
					<h1><g:Label styleName="{style.inline}" ui:field="projectLabel"/></h1>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:FlowPanel ui:field="content" />
			<g:HTMLPanel styleName="{form.formWrapper}">
				<g:HTMLPanel styleName="{form.settingsButtonWrapper}">
					<g:HTMLPanel styleName="{form.fieldLabel}">
						<g:Button ui:field="activateButton" text="Save and Activate" styleName="{form.button} {form.settingsButton} {form.right}"/>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{form.fieldLabel}">
						<g:Button ui:field="saveButton" text="Save" styleName="{form.button} {form.settingsButton}"/>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:SimplePanel>
</ui:UiBinder> 